<config xmlns="http://tail-f.com/ns/config/1.0">
  <branch-service xmlns="http://example.com/branch">
  <branch>
    <name>NYC</name>
    <topology>
      <device>
        <name>sim-isr-4451-1</name>
        <type>
          <role>router</role>
          <model>ISR4K</model>
        </type>
        <policies>
          <loopback>
            <policy-name>LOOPBACK</policy-name>
            <ip-address>1.1.1.1</ip-address>
          </loopback>
          <routing>
            <policy-name>ROUTING</policy-name>
            <as-number>62100</as-number>
            <aggregate-address>
              <ip-address>1.1.1.0</ip-address>
              <ip-mask>255.255.255.0</ip-mask>
            </aggregate-address>
            <aggregate-address>
              <ip-address>2.2.2.0</ip-address>
              <ip-mask>255.255.255.0</ip-mask>
            </aggregate-address>
            <aggregate-address>
              <ip-address>3.3.3.0</ip-address>
              <ip-mask>255.255.255.0</ip-mask>
            </aggregate-address>
            <ebgp>
              <remote-as-number>62323</remote-as-number>
              <peer-ip-address>172.34.21.234</peer-ip-address>
              <carrier-name>Verizon</carrier-name>
            </ebgp>
          </routing>
        </policies>
      </device>
      <device>
        <name>sim-catalyst-3850-1</name>
        <type>
          <role>access-core</role>
          <model>C3850</model>
        </type>
        <policies>
          <switchstack>
            <policy-name>SWITCH-STACK</policy-name>
            <switch-number>1</switch-number>
          </switchstack>
          <loopback>
            <policy-name>LOOPBACK</policy-name>
            <ip-address>1.1.1.2</ip-address>
          </loopback>
        </policies>
      </device>
      <connection>
        <name>router-1-wan</name>
        <side>
          <name>A</name>
          <device>sim-isr-4451-1</device>
          <port>
            <interface-type>GigabitEthernet</interface-type>
            <interface-name>0/0/0</interface-name>
            <ip-address>2.2.2.2</ip-address>
            <ip-mask>255.255.255.0</ip-mask>
          </port>
        </side>
        <type>
          <router-wan>
            <policy-name>ROUTER-WAN</policy-name>
          </router-wan>
        </type>
      </connection>
      <connection>
        <name>accesscore-1-router-1</name>
        <side>
          <name>A</name>
          <device>sim-catalyst-3850-1</device>
          <port>
            <interface-type>GigabitEthernet</interface-type>
            <interface-name>1/1/3</interface-name>
            <ip-address>3.3.3.2</ip-address>
            <ip-mask>255.255.255.254</ip-mask>
            <peer-interface>GigabitEthernet0/0/1</peer-interface>
          </port>
        </side>
        <side>
          <name>B</name>
          <device>sim-isr-4451-1</device>
          <port>
            <interface-type>GigabitEthernet</interface-type>
            <interface-name>0/0/1</interface-name>
            <ip-address>3.3.3.3</ip-address>
            <ip-mask>255.255.255.254</ip-mask>
            <peer-interface>GigabitEthernet1/1/3</peer-interface>
          </port>
        </side>
        <type>
          <core-router>
            <policy-name>CORE-ROUTER</policy-name>
          </core-router>
        </type>
      </connection>
      <connection>
        <name>access-1-general-data-300</name>
        <side>
          <name>A</name>
          <device>sim-catalyst-3850-1</device>
          <port>
            <interface-type>Vlan</interface-type>
            <interface-name>300</interface-name>
          </port>
        </side>
        <type>
          <access>
            <policy-name>ACCESS-GENERAL</policy-name>
            <access-type>DATA</access-type>
            <helper-address>77.77.77.77</helper-address>
            <interface-type>GigabitEthernet</interface-type>
            <interface-name>0/0/1</interface-name>
          </access>
        </type>
      </connection>
      <connection>
        <name>access-1-general-data-400</name>
        <side>
          <name>A</name>
          <device>sim-catalyst-3850-1</device>
          <port>
            <interface-type>Vlan</interface-type>
            <interface-name>400</interface-name>
          </port>
        </side>
        <type>
          <access>
            <policy-name>ACCESS-GENERAL</policy-name>
            <access-type>VOICE</access-type>
            <helper-address>77.77.77.77</helper-address>
            <interface-type>GigabitEthernet</interface-type>
            <interface-name>0/0/1</interface-name>
          </access>
        </type>
      </connection>
      <connection>
        <name>access-1-general-security-399</name>
        <side>
          <name>A</name>
          <device>sim-catalyst-3850-1</device>
          <port>
            <interface-type>Vlan</interface-type>
            <interface-name>399</interface-name>
          </port>
        </side>
        <type>
          <access>
            <policy-name>ACCESS-GENERAL</policy-name>
            <access-type>SECURITY</access-type>
            <helper-address>77.77.77.77</helper-address>
            <interface-type>GigabitEthernet</interface-type>
            <interface-name>0/0/2</interface-name>
          </access>
        </type>
      </connection>
    </topology>
  </branch>
  </branch-service>
</config>
