<config xmlns="http://tail-f.com/ns/config/1.0">
  <branch-service xmlns="http://example.com/branch">
  <branch>
    <name>SFO</name>
    <topology>
      <device>
        <name>sim-isr-4451-1</name>
        <type>
          <role>router</role>
          <model>ISR4K</model>
        </type>
        <policies>
          <loopback>
            <policy-name>LOOPBACK</policy-name>
            <ip-address>1.1.1.1</ip-address>
          </loopback>
        </policies>
      </device>
      <connection>
        <name>router-1-wan</name>
        <side>
          <name>A</name>
          <device>sim-isr-4451-1</device>
          <port>
            <interface-type>GigabitEthernet</interface-type>
            <interface-name>0/0/0</interface-name>
            <ip-address>7.7.7.7</ip-address>
            <ip-mask>255.255.255.0</ip-mask>
          </port>
        </side>
        <type>
          <router-wan>
            <policy-name>ROUTER-WAN</policy-name>
          </router-wan>
        </type>
      </connection>
      <connection>
        <name>core-1-router-1</name>
        <side>
          <name>B</name>
          <device>sim-isr-4451-1</device>
          <port>
            <interface-type>GigabitEthernet</interface-type>
            <interface-name>0/0/1</interface-name>
            <ip-address>4.4.4.1</ip-address>
            <ip-mask>255.255.255.254</ip-mask>
          </port>
        </side>
        <type>
          <core-router>
            <policy-name>CORE-ROUTER</policy-name>
          </core-router>
        </type>
      </connection>
      <connection>
        <name>core-2-router-1</name>
        <side>
          <name>B</name>
          <device>sim-isr-4451-1</device>
          <port>
            <interface-type>GigabitEthernet</interface-type>
            <interface-name>0/0/2</interface-name>
            <ip-address>4.4.4.2</ip-address>
            <ip-mask>255.255.255.254</ip-mask>
          </port>
        </side>
        <type>
          <core-router>
            <policy-name>CORE-ROUTER</policy-name>
          </core-router>
        </type>
      </connection>
      <connection>
        <name>router-interlink</name>
        <side>
          <name>A</name>
          <device>sim-isr-4451-1</device>
          <port>
            <interface-type>GigabitEthernet</interface-type>
            <interface-name>0/1/0</interface-name>
            <ip-address>5.5.5.1</ip-address>
            <ip-mask>255.255.255.254</ip-mask>
          </port>
          <port>
            <interface-type>GigabitEthernet</interface-type>
            <interface-name>0/1/1</interface-name>
            <ip-address>5.5.5.2</ip-address>
            <ip-mask>255.255.255.254</ip-mask>
          </port>
        </side>
        <type>
          <router-interlink>
            <policy-name>ROUTER-INTERLINK</policy-name>
          </router-interlink>
        </type>
      </connection>
    </topology>
  </branch>
  </branch-service>
</config>
