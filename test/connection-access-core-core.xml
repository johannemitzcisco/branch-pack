<config-template xmlns="http://tail-f.com/ns/config/1.0">
  <devices xmlns="http://tail-f.com/ns/ncs">
  <device>
    <name>{$DEVICE-NAME}</name>
      <config>
        <interface xmlns="urn:ios">
          <TenGigabitEthernet foreach="{/branch[name=$SERVICE-NAME]/topology/connection[name=$CONNECTION-NAME]/side/port[interface-type = 'TenGigabitEthernet']">
            <name>{current()/interface-name}</name>
            <description>description</description>
            <switchport>
              <mode>
                <trunk/>
              </mode>
              <trunk>
                <allowed>
                  <vlan>
                    <vlans>1</vlans>
                    <vlans>2</vlans>
                  </vlan>
                </allowed>
              </trunk>
            </switchport>
            <channel-group>
              <number>1</number>
              <mode>active</mode>
            </channel-group>
            <dampening>
            </dampening>
            <load-interval>30</load-interval>
          </TenGigabitEthernet>
          <Port-channel>
            <name>1</name>
            <description>description</description>
            <switchport>
              <mode>
                <trunk/>
              </mode>
              <trunk>
                <allowed>
                  <vlan>
                    <vlans>1</vlans>
                    <vlans>2</vlans>
                  </vlan>
                </allowed>
              </trunk>
            </switchport>
            <dampening>
            </dampening>
          </Port-channel>
        </interface>
      </config>
  </device>
  </devices>
</config-template>
