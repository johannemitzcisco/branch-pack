<config xmlns="http://tail-f.com/ns/config/1.0">
  <branch-service xmlns="http://example.com/branch">
  <branch>
    <name>{$BRANCH-NAME}</name>
    <topology>
      <device>
        <topology-name>router</topology-name>
        <type>
          <role>router</role>
          <model>C6824-X-LE-40G</model>
        </type>
        <policies>
          <registration>
            <policy-name>PNP</policy-name>
          </registration>
          <loopback>
            <policy-name>LOOPBACK</policy-name>
            <interface-number>1</interface-number>
          </loopback>
          <loopback>
            <policy-name>LOOPBACK</policy-name>
            <interface-number>2</interface-number>
          </loopback>
          <loopback>
            <policy-name>LOOPBACK</policy-name>
            <interface-number>3</interface-number>
          </loopback>
          <loopback>
            <policy-name>LOOPBACK</policy-name>
            <interface-number>4</interface-number>
          </loopback>
          <loopback>
            <policy-name>LOOPBACK</policy-name>
            <interface-number>5</interface-number>
          </loopback>
          <ospf>
            <policy-name>OSPF-NO-VLAN</policy-name>
          </ospf>
          <routing>
            <policy-name>ROUTING</policy-name>
          </routing>
        </policies>
      </device>
      <connection>
        <name>router-to-wan</name>
        <side>
          <name>A</name>
          <device></device>
          <port>
            <interface-type>TenGigabitEthernet</interface-type>
            <interface-name>1/2</interface-name>
            <ip-mask>255.255.255.0</ip-mask>
          </port>
        </side>
        <type>
          <router-wan>
            <policy-name>ROUTER-WAN</policy-name>
          </router-wan>
        </type>
      </connection>
    </topology>
  </branch>
  </branch-service>
</config>
